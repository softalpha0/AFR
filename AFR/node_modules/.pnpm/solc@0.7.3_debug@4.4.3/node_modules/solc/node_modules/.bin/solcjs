#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbook/AFR/node_modules/.pnpm/solc@0.7.3_debug@4.4.3/node_modules/solc/node_modules:/Users/macbook/AFR/node_modules/.pnpm/solc@0.7.3_debug@4.4.3/node_modules:/Users/macbook/AFR/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbook/AFR/node_modules/.pnpm/solc@0.7.3_debug@4.4.3/node_modules/solc/node_modules:/Users/macbook/AFR/node_modules/.pnpm/solc@0.7.3_debug@4.4.3/node_modules:/Users/macbook/AFR/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../solcjs" "$@"
else
  exec node  "$basedir/../../solcjs" "$@"
fi
