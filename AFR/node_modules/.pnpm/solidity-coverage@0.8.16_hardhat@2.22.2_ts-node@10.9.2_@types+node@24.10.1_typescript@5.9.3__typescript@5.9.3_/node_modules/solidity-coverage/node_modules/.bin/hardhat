#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbook/AFR/node_modules/.pnpm/hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules/hardhat/internal/cli/node_modules:/Users/macbook/AFR/node_modules/.pnpm/hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules/hardhat/internal/node_modules:/Users/macbook/AFR/node_modules/.pnpm/hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules/hardhat/node_modules:/Users/macbook/AFR/node_modules/.pnpm/hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules:/Users/macbook/AFR/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbook/AFR/node_modules/.pnpm/hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules/hardhat/internal/cli/node_modules:/Users/macbook/AFR/node_modules/.pnpm/hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules/hardhat/internal/node_modules:/Users/macbook/AFR/node_modules/.pnpm/hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules/hardhat/node_modules:/Users/macbook/AFR/node_modules/.pnpm/hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules:/Users/macbook/AFR/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules/hardhat/internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../../../../../hardhat@2.22.2_ts-node@10.9.2_@types+node@24.10.1_typescript@5.9.3__typescript@5.9.3/node_modules/hardhat/internal/cli/bootstrap.js" "$@"
fi
